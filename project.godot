[application]

config/name="DungeonCrawler"
run/main_scene="res://scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

Logger="*res://addons/gs_logger/logger.gd"
InitialSetup="*res://scripts/initial_setup.gd"

[debug]

multirun/number_of_windows=3
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args=""
multirun/other_window_args=""

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/gs_logger/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/multirun/plugin.cfg" )

